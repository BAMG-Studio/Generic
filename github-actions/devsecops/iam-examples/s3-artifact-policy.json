{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:PutObjectAcl",
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::my-org-artifacts",
        "arn:aws:s3:::my-org-artifacts/*"
      ]
    }
  ]
}
